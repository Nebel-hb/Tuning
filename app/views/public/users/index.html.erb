
<h2>Users</h2>
        <table class="table">
          <thead>
            <tr>
              <th>image</th>
              <th>name</th>
              <th colspan="3"></th>
            </tr>
          </thead>
          <tbody><% @users.each do |user| %>
          <% if user.role == "general" %>
            <tr>
              <td>
                <%= attachment_image_tag user, :profile_image, format: 'jpeg', size: "40x40" %>
              </td>
              <td>
                <%= user.name %>
              </td>
              <td>
                <%= link_to user_path(user.id) do %>Show<% end %>
              </td>
            </tr>
           </div>
          </tbody>
          <% end %>
          <% end %>
        </table>
    </div>
  </div>
